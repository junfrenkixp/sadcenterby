extends ../layouts/default

block variables
    - const title = "Жимолость | Питомник растительных культур семьи Пухальских";

block content

    - let catalogTitle = 'Жимолость';
    -
        let catalogItems = [
            {
                'link': './honeysuckle/aurora.html',
                'imgSrc': 'honeysuckle/aurora/aurora-md',
                'name': "'Аврора'"
            },
        ]

    include ../../blocks/components/banner/banner
    +banner("honeysuckle")

    section.catalog
        .container
            .catalog__wrap
                include ../../blocks/modules/aside/aside
                include ../../blocks/modules/catalog/catalog

                +catalog(catalogItems, catalogTitle)

block scripts
    script(src="./js/main.js")
    script(src="./js/vendor.js")
